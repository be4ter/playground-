<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 30.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.playground.model.dao.UserDao">
    <insert id="insert" parameterType="User">
      <![CDATA[
        INSERT INTO T_USER (member_name,mobile1,mobile2,email,email_domain,passwd,reg_date)
        VALUES (#{member_name},#{mobile1},#{mobile2},#{email},#{email_domain},#{passwd},#{reg_date})
        ]]>
    </insert>

    <select id="getUserList" resultType="User">
    <![CDATA[
             SELECT * FROM T_USER
        ]]>
    </select>
</mapper>