<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 30.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.playground.model.dao.TestDao">
    <select id="getTest" resultType="Test">
        <![CDATA[

                  SELECT * FROM T_TEST

        ]]>
    </select>

    <insert id="setTest" parameterType="String" >
        insert into T_TEST(contents)
        values(#{contents})
    </insert>
</mapper>